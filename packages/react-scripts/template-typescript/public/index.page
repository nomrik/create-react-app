<apex:page standardStylesheets="false" showHeader="false" docType="html-5.0" controller="{CONTROLLER_NAME}">

    <html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
    <head>
        <meta charset="UTF-8" />
        <title>{STATIC_RESOURCE_NAME}</title>

        <!--Prod-->
        <{PROD_STYLE_TAG_START}:stylesheet value="{!URLFOR($Resource.{STATIC_RESOURCE_NAME},'css/main.css')}"/{PROD_STYLE_TAG_END}>

        <!--Dev-->
        <{DEV_STYLE_TAG_START}:stylesheet value="{NGROK_URL}/static/css/main.css"/{DEV_STYLE_TAG_END}>

    </head>


    <body>
    <div id="root" class="slds" />
	
    <!--Prod-->
    <{PROD_SCRIPT_TAG_START} src="{!URLFOR($Resource.{STATIC_RESOURCE_NAME},'js/bundle.js')}"/{PROD_SCRIPT_TAG_END}>

    <!--Dev-->
    <{DEV_SCRIPT_TAG_START} src="{NGROK_URL}/static/js/bundle.js"/{DEV_SCRIPT_TAG_END}>

    </body>
        
    </html>
</apex:page>